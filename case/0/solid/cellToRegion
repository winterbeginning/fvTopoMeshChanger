FoamFile
{
    format      ascii;
    class       volScalarField;
    location    "0/solid";
    object      cellToRegion;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [];

internalField   uniform 0;

boundaryField
{
    solid_left
    {
        type            zeroGradient;
    }
    solid_right
    {
        type            zeroGradient;
    }
    wall_bottom
    {
        type            zeroGradient;
    }
    frontAndBack
    {
        type            empty;
    }
    solid_to_gas
    {
        type            extrapolatedCalculated;
        value           uniform 0;
    }
}


