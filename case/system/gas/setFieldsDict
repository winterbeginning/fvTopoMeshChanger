FoamFile
{
    format      ascii;
    class       dictionary;
    object      setFieldsDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

defaultValues
{
    cellProc 0;
}

zones
{
    // Setting values inside a box
    right
    {
        type        box;
        zoneType    cell;

        box         (0.5 0 0) (30 30 30);

        values
        {
            cellProc 1;
        }
    }

   
}

